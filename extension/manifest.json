{
	"manifest_version": 2,
	"name": "My Chrome Extension",
	"version": "1.0",
	"description": "An example Chrome extension using devtools.network API",
	"browser_action": {
		"default_popup": "popup/popup.html",
		"default_title": "Generate Evidence"
	},
	"background": {
		"scripts": ["background.js"]
	},
	"devtools_page": "devtools/panel.html",
	"permissions": [
		"activeTab",
		"storage",
		"downloads",
		"notifications",
		"activeTab",
		"<all_urls>",
		"http://*/*",
		"https://*/*",
		"background",
		"tabs",
		"debugger"
	]
}
